{
  "name":"UserMapper",
  "type":"default",
  "datasource":{
    "result":{
      "name":"userMapper.findUser",
      "type":"mysql",
      "dimension":[
        "name"
      ],
      "metric":[
        "id",
        "sex",
        "phone",
        "email",
        "able_status"
      ],
      "filter":{
        "sex":{
          "value":"${sex}"
        },
        "phone":{
          "value":"${phone}"
        }
      },
      "sort":[
        {
          "id":{
            "value":"asc"
          }
        }
      ],
      "top":{
        "value":"2000"
      }
    }
  }
}